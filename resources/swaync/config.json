{
  "$schema": "/etc/xdg/swaync/configSchema.json",
  "positionX": "right",
  "positionY": "top",
  "layer": "top",
  "cssPriority": "application",
  "control-center-margin-top": 2,
  "control-center-margin-bottom": 2,
  "control-center-margin-right": 1,
  "control-center-margin-left": 0,
  "notification-icon-size": 48,
  "notification-body-image-height": 160,
  "notification-body-image-width": 200,
  "timeout": 6,
  "timeout-low": 4,
  "timeout-critical": 0,
  "fit-to-screen": true,
  "control-center-width": 380,
  "notification-window-width": 366,
  "keyboard-shortcuts": true,
  "image-visibility": "when-available",
  "transition-time": 200,
  "hide-on-clear": false,
  "hide-on-action": true,
  "script-fail-notify": true,
  "widgets": [
    "mpris",
    "title",
    "notifications"
  ],
  "widget-config": {
    "title": {
      "text": "Notifications",
      "clear-all-button": true,
      "button-text": "Clear All (Shift+C)"
    },
    "dnd": {
      "text": "Do Not Disturb (Shift+D)"
    },
    "mpris": {
      "image-size": 96,
      "image-radius": 6
    }},
    "notification-visibility": {
      "1-power-status": {
        "state": "transient",
        "app-name": "poweralertd"
      },
      "2-hud-messages": {
        "state": "transient",
        "category": "hud"
      },
      "3-bluetooth": {
        "state": "transient",
        "app-name": "blueman"
      },
      "4-wifimenu": {
        "state": "transient",
        "app-name": "networkmanager-dmenu"
      },
      "5-everything-else!": {
        "state": "muted",
        "summary": ".*"
      }
    }  
  
}